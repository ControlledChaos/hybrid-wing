{
	"name"       : "rarst/hybrid-wing",
	"description": "Toolkit theme for WordPress, based on Hybrid Core and Twitter Bootstrap.",
	"keywords"   : ["wordpress", "bootstrap", "hybrid"],
	"type"       : "wordpress-theme",
	"homepage"   : "https://github.com/Rarst/hybrid-wing",
	"license"    : "GPL-3.0",
	"authors"    : [
		{
			"name"    : "Andrey Savchenko",
			"email"   : "contact@rarst.net",
			"homepage": "http://www.Rarst.net/"
		}
	],
	"support"    : {
		"issues": "https://github.com/Rarst/hybrid-wing/issues"
	},
	"repositories": [
		{
			"type": "composer",
			"url" : "http://rarst.net"
		}
	],
	"require"    : {
		"php"                : ">=5.3",
		"composer/installers": "~1.0",
		"rarst/hybrid-core"  : "~1.6",
		"rarst/locate-vendor": "~1.0",
		"twbs/bootstrap"     : "~3.0",
		"pimple/pimple"      : "~2.0"
	},
	"autoload"   : {
		"classmap": ["php/"],
		"files"   : ["php/template-tags.php"]
	},
	"extra": {
		"installer-paths": {
			"./hybrid-core/": ["rarst/hybrid-core"]
		}
	}
}